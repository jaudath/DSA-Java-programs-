import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.println("entere the value of n ");
        int n = scan.nextInt();
        int fact = 1; 
        int i ;
        int d ;
        int sum = 0 ; 

        for( i=1 ; i<=n;i++){
            fact =fact *i;
            
        }
        while(fact>0){
            d=fact%10;
            fact = fact / 10;
            if (d==0){
                sum++;
            }
            
            
        }
        System.out.print(sum);
    }
}
